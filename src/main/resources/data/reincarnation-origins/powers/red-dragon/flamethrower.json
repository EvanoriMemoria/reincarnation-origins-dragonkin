{
  "type": "origins:multiple",
  "flamethrower": {
    "type": "origins:active_self",
    "entity_action": {
      "type": "origins:raycast",
      "distance": 12,
      "block": true,
      "shape_type": "collider",
      "fluid_handling": "any",
      "block_action": {
        "type": "origins:and",
        "actions": [
          {
            "type": "origins:execute_command",
            "command": "data merge block ~ ~ ~1 {BurnTime:6400}"
          },
          {
            "type": "origins:execute_command",
            "command": "fill ~-2 ~ ~ ~2 ~2 ~ minecraft:fire replace minecraft:air"
          }
        ]
      },
      "bientity_action": {
        "type": "origins:target_action",
        "action": {
          "type": "origins:and",
          "actions": [
            {
              "type": "origins:damage",
              "amount": 2,
              "source": {
                "name": "onFire.player",
                "bypasses_armor": true
              }
            },
            {
              "type": "origins:set_on_fire",
              "duration": 5
            }
          ]
        }
      },
      "before_action": {
        "type": "origins:and",
        "actions": [
          {
            "type": "origins:exhaust",
            "amount": 0.1
          },
          {
            "type": "origins:change_resource",
            "resource": "reincarnation-origins:red-dragon/fuel_fuelbar",
            "change": -6
          }
        ]
      },
      "command_along_ray": "particle minecraft:flame ~ ~-0.15 ~ .01 .01 .01 0 2 force",
      "command_step": 0.33
    },
    "cooldown": 5,
    "hud_render": {
      "should_render": false
    },
    "key": {
      "key": "key.origins.primary_active",
      "continuous": true
    }
  }
}
