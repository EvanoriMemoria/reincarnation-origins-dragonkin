{
  "type": "origins:multiple",
  "slippery": {
    "type": "origins:modify_slipperiness",
    "modifier": {
      "operation": "multiply_total",
      "value": 1.5
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:on_block"
        },
        {
          "type": "origins:swimming"
        },
        {
          "type": "origins:fluid_height",
          "fluid": "minecraft:water",
          "comparison": "==",
          "compare_to": 0.0
        }
      ]
    }
  },
  "belly_slide": {
    "type": "origins:swimming",
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:fluid_height",
          "fluid": "minecraft:water",
          "comparison": "==",
          "compare_to": 0.0
        },
        {
          "type": "origins:resource",
          "resource": "*:*_timer",
          "comparison": "<",
          "compare_to": 100
        }
      ]
    }
  },
  "timer": {
    "type": "origins:resource",
    "min": 0,
    "max": 120,
    "hud_render": {
      "should_render": false
    }
  },
  "timer_counter": {
    "type": "origins:action_over_time",
    "interval": 5,
    "entity_action": {
      "type": "origins:if_else",
      "condition": {
        "type": "origins:on_block",
        "inverted": true
      },
      "if_action": {
        "type": "origins:change_resource",
        "resource": "*:*_timer",
        "operation": "add",
        "change": 5
      },
      "else_action": {
        "type": "origins:change_resource",
        "resource": "*:*_timer",
        "operation": "set",
        "change": 0
      }
    }
  },
  "fake_gravity": {
    "type": "origins:action_over_time",
    "interval": 1,
    "entity_action": {
      "type": "origins:add_velocity",
      "set": true,
      "y": -0.24
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:fluid_height",
          "fluid": "minecraft:water",
          "comparison": "==",
          "compare_to": 0.0
        },
        {
          "type": "origins:swimming"
        },
        {
          "type": "origins:block_collision",
          "offset_x": 0,
          "offset_y": -0.65,
          "offset_z": 0,
          "inverted": true
        }
      ]
    }
  },
  "fast-swim-land": {
    "type": "origins:modify_swim_speed",
    "modifier": {
      "operation": "multiply_base",
      "value": 1.0
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:fluid_height",
          "fluid": "minecraft:water",
          "comparison": "==",
          "compare_to": 0.0
        },
        {
          "type": "origins:on_block"
        }
      ]
    }
  },
  "step-height": {
    "type": "origins:action_on_callback",
    "entity_action_added": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:step_height 2"
    },
    "entity_action_removed": {
      "type": "origins:execute_command",
      "command": "scale reset pehkui:step_height"
    }
  },
  "splashy-slide": {
    "type": "origins:action_over_time",
    "interval": 5,
    "entity_action": {
      "type": "origins:execute_command",
      "command": "particle minecraft:splash ~ ~ ~ 1 0 1 1 50 normal"
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:on_block"
        },
        {
          "type": "origins:swimming"
        },
        {
          "type": "origins:fluid_height",
          "fluid": "minecraft:water",
          "comparison": "==",
          "compare_to": 0.0
        }
      ]
    }
  }
}
