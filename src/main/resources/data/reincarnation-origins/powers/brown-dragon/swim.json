{
  "type": "origins:multiple",
  "phase": {
    "type": "origins:phasing",
    "condition": {
      "type": "origins:power_active",
      "power": "reincarnation-origins:brown-dragon/swim_toggle"
    },
    "blacklist": false,
    "render_type": "blindness",
    "view_distance": 10,
    "block_condition": {
      "type": "origins:in_tag",
      "tag": "minecraft:base_stone_overworld"
    },
    "phase_down_condition": {
      "type": "origins:or",
      "conditions": [
        {
          "type": "origins:and",
          "conditions": [
            {
              "type": "origins:sneaking"
            },
            {
              "type": "origins:on_block"
            }
          ]
        },
        {
          "type": "origins:swimming"
        }
      ]
    }
  },
  "exhaustion": {
    "type": "origins:exhaust",
    "interval": 20,
    "exhaustion": 0.7,
    "condition": {
      "type": "origins:power_active",
      "power": "reincarnation-origins:brown-dragon/swim_toggle"
    }
  },
  "magic_drain": {
    "type": "origins:action_over_time",
    "interval": 20,
    "entity_action": {
      "type": "origins:change_resource",
      "resource": "reincarnation-origins:brown-dragon/fuel_fuelbar",
      "change": -1
    },
    "condition": {
      "type": "origins:power_active",
      "power": "reincarnation-origins:brown-dragon/swim_toggle"
    }
  },
  "stone_swim": {
    "type": "origins:swimming",
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:fluid_height",
          "fluid": "minecraft:water",
          "comparison": ">",
          "compare_to": 0.0,
          "inverted": true
        },
        {
          "type": "origins:in_block",
          "block_condition": {
            "type": "origins:in_tag",
            "tag": "reincarnation-origins:expanded_dig"
          }
        }
      ]
    }
  },
  "toggle": {
    "type": "origins:toggle",
    "active_by_default": false,
    "key": {
      "key": "key.saveToolbarActivator"
    }
  },
  "slow_falling_in_stone": {
    "type": "origins:modify_falling",
    "velocity": 0.015,
    "condition": {
      "type": "origins:in_block",
      "block_condition": {
        "type": "origins:in_tag",
        "tag": "minecraft:base_stone_overworld"
      }
    }
  },
  "passive_souls": {
    "type": "origins:action_over_time",
    "interval": 30,
    "condition": {
      "type": "origins:power_active",
      "power": "reincarnation-origins:brown-dragon/swim_toggle"
    },
    "entity_action": {
      "type": "origins:execute_command",
      "command": "particle minecraft:soul ~ ~ ~ 5 5 5 0 10 normal"
    }
  },
  "fast-swim-ground": {
    "type": "origins:modify_swim_speed",
    "modifier": {
      "operation": "multiply_base",
      "value": 1.0
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:fluid_height",
          "fluid": "minecraft:water",
          "comparison": "==",
          "compare_to": 0.0
        }
      ]
    }
  },
  "no_suffocation": {
    "type": "origins:invulnerability",
    "damage_condition": {
      "type": "origins:name",
      "name": "inWall"
    }
  },
  "condition": {
    "type": "origins:power_active",
    "power": "reincarnation-origins:brown-dragon/swim_toggle"
  }
}
