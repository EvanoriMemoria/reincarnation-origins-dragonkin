{
  "type": "origins:multiple",
  "stomp": {
    "type": "origins:active_self",
    "key": "key.origins.primary_active",
    "condition": {
      "type": "origins:resource",
      "resource": "*:*_fuelbar",
      "comparison": ">",
      "compare_to": 0
    },
    "entity_action": {
      "type": "origins:area_of_effect",
      "radius": 8,
      "bientity_action": {
        "type": "origins:and",
        "actions": [
          {
            "type": "origins:add_velocity",
            "client": true,
            "z": 1.6,
            "y": 0.6
          },
          {
            "type": "apply_effects",
            "effects": {
              "effect": "minecraft:slowness",
              "amplifier": 9,
              "duration": 100,
              "is_ambient": true,
              "show_particles": false,
              "show_icon": true
            }
          },
          {
            "type": "origins:damage",
            "amount": 10,
            "damage_type": "minecraft:in_wall"
          },
          {
            "type": "origins:spawn_particles",
            "particle": {
              "type": "minecraft:poof",
              "params": "0.922 0.314 0.314 2"
            },
            "count": 4,
            "speed": 0.0,
            "force": true,
            "spread": {
              "x": 1.0,
              "y": 0.0,
              "z": 1.0
            }
          },
          {
            "type": "origins:execute_command",
            "command": "power grant @s reincarnation-origins:support/petrify_color"
          },
          {
            "type": "origins:delay",
            "ticks": 100,
            "action": {
              "type": "origins:execute_command",
              "command": "power revoke @s reincarnation-origins:support/petrify_color"
            }
          }
        ]
      }
    },
    "before_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:exhaust",
          "amount": 0.1
        },
        {
          "type": "origins:change_resource",
          "resource": "*:*_fuelbar",
          "change": -1
        }
      ]
    }
  },
  "fuelbar": {
    "type": "origins:resource",
    "min": 0,
    "max": 6,
    "min_action": {
      "type": "origins:execute_command",
      "command": "function reincarnation-origins:dragonkin/transform_into_dragonkin"
    },
    "hud_render": {
      "sprite_location": "origins:textures/gui/resource_bar.png",
      "bar_index": 7,
      "should_render": true
    }
  },
  "fuel_regen": {
    "_comment": "Regenerate Fuel Once Every twenty Seconds while not under the sky",
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:change_resource",
      "resource": "*:*_fuelbar",
      "change": 1
    },
    "condition": {
      "type": "origins:exposed_to_sky",
      "inverted": true
    },
    "interval": 400
  },
  "fuel_regen_conditional": {
    "_comment": "Regenerate an additional fuel Every five Seconds, while near enough natural stone",
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:change_resource",
      "resource": "*:*_fuelbar",
      "change": 1
    },
    "condition": {
      "type": "origins:block_in_radius",
      "block_condition": {
        "type": "origins:in_tag",
        "tag": "minecraft:base_stone_overworld"
      },
      "radius": 4,
      "shape": "cube",
      "comparison": ">=",
      "compare_to": 5
    },
    "interval": 100
  },
  "fuel_degen": {
    "_comment": "Lose fuel while under the sky",
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:change_resource",
      "resource": "*:*_fuelbar",
      "change": -1
    },
    "interval": 200,
    "condition": {
      "type": "origins:exposed_to_sky"
    }
  }
}
