{
  "type": "origins:multiple",
  "delay": {
    "type": "origins:resource",
    "min": 0,
    "max": 1
  },
  "command": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:if_else",
      "condition": {
        "type": "origins:resource",
        "resource": "reincarnation-origins:brown-dragon/highlight_support/timer_delay",
        "comparison": "==",
        "compare_to": 0
      },
      "if_action": {
        "type": "origins:execute_command",
        "command": "tp @s ~ -200 ~"
      },
      "else_action": {
        "type": "origins:change_resource",
        "resource": "reincarnation-origins:brown-dragon/highlight_support/timer_delay",
        "change": -1
      }
    },
    "interval": 300
  }
}
