{
  "type": "origins:action_over_time",
  "entity_action": {
    "type": "origins:if_else",
    "condition": {
      "type": "origins:block_in_radius",
      "block_condition": {
        "type": "origins:in_tag",
        "tag": "reincarnation-origins:non_stone_blocks"
      },
      "radius": 0,
      "shape": "cube",
      "comparison": ">=",
      "compare_to": 1
    },
    "if_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:delay",
          "ticks": 10,
          "action": {
            "type": "origins:execute_command",
            "command": "kill @s"
          }
        }
      ]
    },
    "else_action": {
      "type": "origins:execute_command",
      "command": "kill @s"
    }
  },
  "interval": 1
}
