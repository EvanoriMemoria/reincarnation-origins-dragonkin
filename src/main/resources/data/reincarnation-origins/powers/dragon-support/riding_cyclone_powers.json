{
  "type": "origins:multiple",
  "description": "this goes on the white dragon cyclone being ridden",
  "hidden": true,
  "particles_still": {
    "type": "origins:action_over_time",
    "interval": 1,
    "entity_action": {
      "type": "origins:execute_command",
      "command": "particle poof ~ ~ ~ 0.1 0.5 0.1 0.1 10 force"
    },
    "condition": {
      "type": "origins:moving",
      "inverted": true
    }
  },
  "particles_moving": {
    "type": "origins:action_over_time",
    "interval": 1,
    "entity_action": {
      "type": "origins:execute_command",
      "command": "particle poof ^ ^ ^0.5 0.1 0.5 0.1 0.1 10 force"
    },
    "condition": {
      "type": "origins:moving"
    }
  },
  "glide": {
    "type": "origins:modify_falling",
    "velocity": 0.02,
    "take_fall_damage": false
  },
  "kill_on_dismount": {
    "type": "origins:action_over_time",
    "interval": 5,
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "particle poof ~ ~ ~ 1 1 1 0.4 50 force"
        },
        {
          "type": "origins:execute_command",
          "command": "tp @s ~ ~-200 ~"
        },
        {
          "type": "origins:execute_command",
          "command": "kill @s"
        }
      ]
    },
    "condition": {
      "type": "origins:or",
      "conditions": [
        {
          "type": "origins:passenger",
          "comparison": "==",
          "compare_to": "0",
          "bientity_condition": {
            "type": "origins:actor_condition",
            "condition": {
              "type": "origins:entity_type",
              "entity_type": "minecraft:player"
            }
          }
        },
        {
          "type": "origins:fluid_height",
          "fluid": "minecraft:lava",
          "comparison": ">",
          "compare_to": 0.4
        },
        {
          "type": "origins:fluid_height",
          "fluid": "minecraft:water",
          "comparison": ">",
          "compare_to": 0.4
        }
      ]
    }
  },
  "invisible": {
    "type": "origins:invisibility",
    "render_armor": false
  },
  "float_over_lava": {
    "type": "origins:walk_on_fluid",
    "fluid": "minecraft:lava",
    "condition": {
      "type": "origins:fluid_height",
      "fluid": "minecraft:lava",
      "comparison": "<=",
      "compare_to": 0.4
    }
  },
  "float_over_water": {
    "type": "origins:walk_on_fluid",
    "fluid": "minecraft:water",
    "condition": {
      "type": "origins:fluid_height",
      "fluid": "minecraft:water",
      "comparison": "<=",
      "compare_to": 0.4
    }
  },
  "magic_drain": {
    "type": "origins:action_over_time",
    "interval": 20,
    "entity_action": {
      "type": "origins:execute_command",
      "command": "resource change @p reincarnation-origins:white-dragon/fuel_fuelbar -1"
    },
    "condition": {
      "type": "origins:moving",
      "inverted": true
    }
  },
  "magic_drain_moving": {
    "type": "origins:action_over_time",
    "interval": 20,
    "entity_action": {
      "type": "origins:execute_command",
      "command": "resource change @p reincarnation-origins:white-dragon/fuel_fuelbar -4"
    },
    "condition": {
      "type": "origins:moving"
    }
  }
}
